<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>

<BorderPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.controller.ItemController">
   <center>
      <HBox alignment="CENTER" spacing="5.0">
         <children>
            <Label layoutX="38.0" layoutY="19.0" visible="false" />
            <Label fx:id="text" alignment="CENTER" minWidth="180.0" text="Label" textAlignment="CENTER" HBox.hgrow="ALWAYS" />
            <Button fx:id="remove" alignment="CENTER_RIGHT" mnemonicParsing="false" onAction="#handleRemove" text="Remove" HBox.hgrow="NEVER">
               <HBox.margin>
                  <Insets />
               </HBox.margin>
            </Button>
         </children>
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding>
      </HBox>
   </center>
</BorderPane>
